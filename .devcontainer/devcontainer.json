{
   "name": "NEAR CLI Devcontainer",
   "postStartCommand": "echo 'export PS1=\\\"> \\\"' >> ~/.bashrc",
   "build":{
      "dockerfile":"./Dockerfile",
      "context":"."
   },
   "customizations":{
      "vscode":{
         "extensions":[
            "rust-lang.rust-analyzer",
            "serayuzgur.crates",
            "tamasfe.even-better-toml",
            "artdiniz.quitcontrol-vscode"
         ],
         "settings":{
            "workbench.startupEditor":"none"
         }
      }
   }
}
